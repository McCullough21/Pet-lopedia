<%= form_for @breed do |f| %>
    <%=f.label :name %>
    <%=f.text_field :name %>
    <br>
    <%=f.label :female_height %>
    <%=f.number_field :female_height %>
    <br>
    <%=f.label :female_weight %>
    <%=f.number_field :female_weight %>
    <br>
    <%=f.label :male_height %>
    <%=f.number_field :male_height %>
    <br>
    <%=f.label :male_height %>
    <%=f.number_field :male_height %>
    <br>
    <%=f.label :description%>
    <%=f.text_area :description%>
    
     
    <%= f.submit "Create" %>

<% end %>