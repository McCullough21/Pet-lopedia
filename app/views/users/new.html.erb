<%= form_for @user  do |f| %>
    
    <%= f.label :username %><br>
    <%= f.text_field :username %>
    <br>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
    <br>
    <%= f.label "Qualification" %><br>
    <br>
    <%= f.collection_check_boxes :qualification_ids, Qualification.all, :id, :profession %>
    <br>
    <br>
    <%= f.submit "Create" %>
<% end %>
