<%= form_for @user  do |f| %>
    
    <%= f.label :username %><br>
    <%= f.text_field :username %>
    <br>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
    <br>
    <%= f.label "Do you have experience as:" %><br>
    <br>
    <%= f.select :qualification, [ 'Veterinarian', 'Vet-Tech', 'ASPCA', 'Breeder'], :prompt => 'Select' %>
    <br>
    <br>
    <%= f.submit "Create" %>
<% end %>
